{
  "name": "simple-web-terminal",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "dev": "pnpm build:server && node dist/server.js",
    "dev:watch": "concurrently \"pnpm build:server:watch\" \"nodemon dist/server.js\"",
    "build": "pnpm build:server && next build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server:watch": "tsc -p tsconfig.server.json --watch",
    "start": "NODE_ENV=production pnpm build:server && NODE_ENV=production node dist/server.js",
    "lint": "eslint",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.{js,jsx,ts,tsx}' --coverageReporters=text-lcov --coverageReporters=html",
    "test:unit": "jest __tests__/components __tests__/lib src/components/__tests__ src/lib/__tests__ --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.js __tests__/integration __tests__/api --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "jest __tests__/accessibility --passWithNoTests",
    "test:performance": "jest __tests__/performance --passWithNoTests",
    "test:all": "node scripts/test-all.js",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/crypto-js": "^4.2.2",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.541.0",
    "multer": "^2.0.2",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "node-forge": "^1.3.1",
    "node-ssh": "^13.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "ssh-keygen": "^0.5.0",
    "ssh2-sftp-client": "^12.0.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "ws": "^8.18.3",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.5",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
